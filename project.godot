; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Gwj 65"
run/main_scene="res://scenes/main_menu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://assets/graphics/icon.svg"

[autoload]

Global="*res://scripts/global.gd"
UserSettings="*res://scripts/user_settings.gd"
AudioBus="*res://scenes/audio_bus.tscn"
Player="*res://scripts/player.gd"
EnemyHelper="*res://scripts/enemy_helper.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720

[filesystem]

import/blender/enabled=false

[gui]

theme/custom="res://theme.tres"

[internationalization]

locale/translations=PackedStringArray("res://text/ui.en.translation", "res://text/ui.ru.translation")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
